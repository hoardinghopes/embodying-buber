{
  "name": "embodying-buber",
  "version": "2.0.0",
  "description": "Helping the body to make sense of Martin Buber",
  "author": {
    "name": "James Knight",
    "email": "tech@jamesknightcoaching.com"
  },
  "license": "MIT",
  "browserslist": [
    "last 3 Chrome versions",
    "last 3 Firefox versions",
    "last 3 Safari versions",
    "last 3 Edge versions"
  ],
  "scripts": {
    "dev": "concurrently pnpm:dev:*",
    "dev:11ty": "NODE_ENV=development eleventy --serve",
    "dev:mix": "mix watch",
    "mix": "mix",
    "prod": "pnpm run production",
    "production": "pnpm run prod:mix && pnpm run prod:11ty",
    "prod:11ty": "NODE_ENV=production eleventy --serve",
    "prod:mix": "mix --production",
    "preview": "pnpm run prod:mix && NODE_ENV=production eleventy --serve",
    "stage": "pnpm run stage:mix && pnpm run stage:eleventy",
    "stage:eleventy": "NODE_ENV=stage eleventy",
    "stage:mix": "mix --production",
    "stage:serve": "pnpm run stage:mix && NODE_ENV=stage eleventy --serve",
    "clean": "rm -rf _site",
    "biome": "pnpm run format && pnpm run lint",
    "format": "biome format --write .",
    "format:test": "biome format  .",
    "lint": "biome check  ."
  },
  "devDependencies": {
    "@11ty/eleventy-plugin-rss": "^2.0.2",
    "@biomejs/biome": "^1.3.1",
    "@fabiospampinato/bump": "^3.2.0",
    "acclaimed": "^1.0.12",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-css": "^5.3.2",
    "clean-webpack-plugin": "^4.0.0",
    "cloudinary": "^1.41.0",
    "concurrently": "8.2.1",
    "eleventy-critical-css": "^1.1.0",
    "eleventy-plugin-metagen": "^1.8.3",
    "eleventy-plugin-purgecss": "^0.4.0",
    "eleventy-plugin-reading-time": "^0.0.1",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "laravel-mix": "^6.0.49",
    "laravel-mix-clean": "^0.1.0",
    "laravel-mix-versionhash": "^2.0.1",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^13.3.2",
    "serve": "14.2.1",
    "webpack": "^5.89.0"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@caneara/iodine": "^8.5.0",
    "@tbranyen/jsdom": "^13.0.0",
    "alpinejs": "^3.13.2",
    "gitlog": "^4.0.8",
    "html-minifier": "^4.0.0",
    "luxon": "^3.4.3",
    "markdown-it-external-links": "^0.0.6",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-wikilinks": "^1.4.0",
    "sass": "^1.69.5",
    "uuid": "^9.0.1",
    "webpack-cli": "^4.10.0"
  }
}